@model List<string>;
@{
    Layout = "_Layout";
}

@section styles
{
    <link href="/css/generationLookup.css" rel="stylesheet" type="text/css" />
}

@section scripts
{
    <script src="/js/generationLookup.js" type="text/javascript"></script>
}

<p class="adminButtons">
    @if(User.IsInRole("Owner"))
    {
        <a class="btn btn-primary" href="@Url.Action("Pokemon", "Add")" role="button">Add Pokemon</a>
    }
</p>
<div class="generationList">
    @foreach(var id in Model)
    {
        <button id="Generation@(id)" onclick="openAdminGeneration('@id')">Generation @id</button>
    }
</div>
<div class="generation pokemonlist">
    <div class="grid-container"></div>
</div>